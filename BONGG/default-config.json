{
    "$schema": "https://raw.githubusercontent.com/ow-mods/owml/master/schemas/config_schema.json",
    "enabled": true,
    "volume": 0.5,
    "showDebugColliders": false,
    "settings": {
        "enabled": {
            "type": "toggle",
            "value": true,
            "title": "Enable Bell Sounds"
        },
        "volume": {
            "type": "slider",
            "value": 0.5,
            "min": 0.0,
            "max": 1.0,
            "title": "Bell Volume"
        },
        "showDebugColliders": {
            "type": "toggle",
            "value": false,
            "title": "Show Debug Colliders"
        }
    }
}